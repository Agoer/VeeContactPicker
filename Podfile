# Podfile VeeContactPicker
source 'https://github.com/CocoaPods/Specs.git'

#Add the following to the top of Podfile to tell CocoaPods to silence all warnings from pod dependencies:
inhibit_all_warnings!

target 'VeeContactPicker', :exclusive => true do
	platform :ios, '7.0'

	#Contact's image placeholders
	pod 'UIImageView-AGCInitials'
end

target 'VeeContactPickerTests', :exclusive => true do
	platform :ios, '7.0'
 	pod 'OCMock'
end

target 'VeeContactPickerUITests', :exclusive => true do
	platform :ios, '7.0'
end

